				brcm,pins = <0x09 0x0a 0x0b>;
			};

			spi4_cs_pins {
				brcm,function = <0x01>;
				phandle = <0x1f>;
				brcm,pins = <0x04 0x19>;
			};

			spi5-gpio12 {
				phandle = <0xa6>;

				pins-spi {
					function = "alt3";
					pins = "gpio12\0gpio13\0gpio14\0gpio15";
				};
			};

			uart5-ctsrts-gpio14 {
				phandle = <0xaf>;

				pin-cts {
					function = "alt4";
					pins = "gpio14";
					bias-pull-up;
				};

				pin-rts {
					function = "alt4";
					pins = "gpio15";
					bias-disable;
				};
			};

			jtag-gpio22 {
				brcm,function = <0x03>;
				phandle = <0x70>;
				brcm,pins = <0x16 0x17 0x18 0x19 0x1a 0x1b>;
			};

			spi6_pins {
				brcm,function = <0x07>;
				phandle = <0x22>;
				brcm,pins = <0x13 0x14 0x15>;
			};

			pwm0-1-gpio45 {
				phandle = <0x9a>;

				pin-pwm {
					function = "alt0";
					pins = "gpio45";
					bias-disable;
				};
			};

			spi0-gpio35 {
				brcm,function = <0x04>;
				phandle = <0x75>;
				brcm,pins = <0x23 0x24 0x25 0x26 0x27>;
			};

			uart1_bt_pins {
				brcm,pull = <0x00 0x02 0x02 0x00>;
				brcm,function = <0x02>;
				phandle = <0xbb>;
				brcm,pins = <0x20 0x21 0x1e 0x1f>;
			};

			emmc-gpio34 {
				brcm,pull = <0x00 0x02 0x02 0x02 0x02 0x02>;
				brcm,function = <0x07>;
				phandle = <0x66>;
				brcm,pins = <0x22 0x23 0x24 0x25 0x26 0x27>;
			};

			bt_pins {
				brcm,pull;
				brcm,function;
				phandle = <0x0a>;
				brcm,pins;
			};

			uart0-gpio36 {
				brcm,function = <0x06>;
				phandle = <0x7c>;
				brcm,pins = <0x24 0x25>;
			};

			i2c5 {
				brcm,pull = <0x02>;
				brcm,function = <0x02>;
				phandle = <0x26>;
				brcm,pins = <0x0c 0x0d>;
			};

			pwm0-1-gpio53 {
				phandle = <0x9c>;

				pin-pwm {
					function = "alt1";
					pins = "gpio53";
					bias-disable;
				};
			};

			spi6_cs_pins {
				brcm,function = <0x01>;
				phandle = <0x23>;
				brcm,pins = <0x12 0x1b>;
			};

			i2c1-gpio2 {
				brcm,function = <0x04>;
				phandle = <0x6e>;
				brcm,pins = <0x02 0x03>;
			};

		uart0_pins:	uart0_pins {
				brcm,pull;
				brcm,function;
				phandle = <0x09>;
				brcm,pins;
			};

			gpclk2-gpio6 {
				brcm,function = <0x04>;
				phandle = <0x6b>;
				brcm,pins = <0x06>;
			};

			gpclk2-gpio43 {
				brcm,pull = <0x00>;
				brcm,function = <0x04>;
				phandle = <0x6c>;
				brcm,pins = <0x2b>;
			};

			audio_pins {
				brcm,pull = <0x00>;
				brcm,function = <0x04>;
				phandle = <0x36>;
				brcm,pins = <0x28 0x29>;
			};

			gpioout {
				brcm,function = <0x01>;
				phandle = <0xb8>;
				brcm,pins = <0x06>;
			};


		spi1_int_pins:	int_pins {
				brcm,pins = <0x18>;
				brcm,function = <0x00>;
				brcm,pull = <0x00>;
				phandle = <0xFA>;
			};

		};

	uart0:	serial@7e201000 {
			compatible = "arm,pl011\0arm,primecell";
			reg = <0x7e201000 0x200>;
			interrupts = <0x00 0x79 0x04>;
			clocks = <&cprman 0x13 &cprman 0x14>;
			clock-names = "uartclk\0apb_pclk";
			arm,primecell-periphid = <0x241011>;
			status = "okay";
			cts-event-workaround;
			pinctrl-names = "default";
			pinctrl-0 = <&uart0_pins>;
			uart-has-rtscts;
			skip-init;
			phandle = <0x43>;
		};

	aux:	aux@7e215000 {
			clocks = <&cprman 0x14>;
			#clock-cells = <0x01>;
			compatible = "brcm,bcm2835-aux";
			reg = <0x7e215000 0x08>;
			phandle = <0x11>;
		};

	uart1:	serial@7e215040 {
			compatible = "brcm,bcm2835-aux-uart";
			reg = <0x7e215040 0x40>;
			interrupts = <0x00 0x5d 0x04>;
			//clocks = <&clk_aux_uart 0x00>;
			clocks = <&aux 0x00>;
			status = "disabled";
			pinctrl-names = "default";
			pinctrl-0 = <&uart1_pins>;
			skip-init;
			phandle = <0x44>;
		};

	uart5:	serial@7e201a00 {
			compatible = "arm,pl011\0arm,primecell";
			reg = <0x7e201a00 0x200>;
			interrupts = <0x00 0x79 0x04>;
			clocks = <&cprman 0x13 &cprman 0x14>;
			clock-names = "uartclk\0apb_pclk";
			arm,primecell-periphid = <0x241011>;
			status = "okay";
			pinctrl-0 = <&uart5_pins>;
			pinctrl-names = "default";
			phandle = <0xc7>;
		};


	spi1:	spi@7e215080 {
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			compatible = "brcm,bcm2835-aux-spi";
			reg = <0x7e215080 0x40>;
			pinctrl-names = "default";
			pinctrl-0 = <&spi1_pins &spi1_cs_pins>;
			interrupts = <0x00 0x5d 0x04>;
			clocks = <&aux 0x01>;
			status = "okay";
			phandle = <0xbf>;
			cs-gpios = <&gpio 0x12 0x01>;

		sc16is752:	sc16is752@0 {
				compatible = "nxp,sc16is752";
				reg = <0x00>;
				clocks = <&clk_sc16is752>;
				interrupt-parent = <&gpio>;
				//interrupts = <&spi1_int_pins 0x02>;
				interrupts = <0x18 0x02>;
				//pinctrl-0 = <&spi1_int_pins>;
				//pinctrl-names = "default";
				gpio-controller;
				#gpio-cells = <0x02>;
				spi-max-frequency = "\0=\t";
				//spi-max-frequency = "10";
				//spi-max-frequency = "400";
				phandle = <0x100>;
			};
		};



//	spi3:	spi@7e204600 {
//			pinctrl-names = "default";
//			#address-cells = <0x01>;
//			pinctrl-0 = <&spi3_pins &spi3_cs_pins>;
//			interrupts = <0x00 0x76 0x04>;
//			clocks = <&cprman 0x14>;
//			#size-cells = <0x00>;
//			compatible = "brcm,bcm2835-spi";
//			status = "disabled";
//			reg = <0x7e204600 0x200>;
//			phandle = <0xca>;
//		};

		spi0: spi@7e204000 {
			compatible = "brcm,bcm2835-spi";
			reg = <0x7e204000 0x200>;
			#address-cells = <0x01>;
			pinctrl-names = "default";
			pinctrl-0 = <&spi0_pins &spi0_cs_pins>;
			interrupts = <0x00 0x76 0x04>;
			clocks = <&cprman 0x14>;
			#size-cells = <0x00>;
			status = "okay";
			phandle = <0x46>;
			//dmas = <&dma 0x06 &dma 0x07>;
			dma-names = "tx\0rx";
			dmas = <&dma 0x06 &dma 0x07>;
			cs-gpios = <&gpio 0x08 0x01 &gpio 0x07 0x01>;

		spidev0:	spidev@0 {
				#address-cells = <0x01>;
				#size-cells = <0x00>;
				spi-max-frequency = <0x7735940>;
				compatible = "spidev";
				reg = <0x00>;
				phandle = <0xbc>;
			};

		spidev1:	spidev@1 {
				#address-cells = <0x01>;
				#size-cells = <0x00>;
				spi-max-frequency = <0x7735940>;
				compatible = "spidev";
				reg = <0x01>;
				phandle = <0xbd>;
			};
		};


	dma:	dma-controller@7e007000 {
			interrupts = <0x00 0x50 0x04 0x00 0x51 0x04 0x00 0x52 0x04 0x00 0x53 0x04 0x00 0x54 0x04 0x00 0x55 0x04 0x00 0x56 0x04 0x00 0x57 0x04 0x00 0x57 0x04 0x00 0x58 0x04 0x00 0x58 0x04>;
			brcm,dma-channel-mask = <0x7f5>;
			compatible = "brcm,bcm2835-dma";
			interrupt-names = "dma0\0dma1\0dma2\0dma3\0dma4\0dma5\0dma6\0dma7\0dma8\0dma9\0dma10";
			reg = <0x7e007000 0xb00>;
			phandle = <0x0c>;
			#dma-cells = <0x01>;
		};

/* I2C */

// i2c_arm = "/soc/i2c@7e804000"; (i2c1)
	i2c_arm:	i2c@7e804000 {
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			compatible = "brcm,bcm2711-i2c\0brcm,bcm2835-i2c";
			reg = <0x7e804000 0x1000>;
			pinctrl-names = "default";
			pinctrl-0 = <&i2c1_pins>;
			interrupts = <0x00 0x75 0x04>;
			//clocks = <&cprman &firmware_clocks>;
			clocks = <&cprman 0x14>;
			clock-frequency = <0x61a80>; // 400 KHz
			status = "okay";
			phandle = <0x48>;
		};

// i2c_vc = "/soc/i2c0mux/i2c@0";  i2c0 = "/soc/i2c0mux/i2c@0";

	i2c0mux:	i2c0mux {
			compatible = "i2c-mux-pinctrl";
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			pinctrl-names = "i2c0\0i2c_csi_dsi";
			pinctrl-0 = <&i2c0_gpio0>;
			pinctrl-1 = <&i2c0_gpio44>;
			status = "okay";
			i2c-parent = <&i2c0if>;
			phandle = <0x47>;

		i2c0:	i2c@0 {
				#address-cells = <0x01>;
				#size-cells = <0x00>;
				reg = <0x00>;
				phandle = <0xdb>;
			};

		i2c_csi_dsi:	i2c@1 {
				#address-cells = <0x01>;
				#size-cells = <0x00>;
				status = "okay";
				reg = <0x01>;
				phandle = <0xdc>;

				dw9817@c {
					compatible = "dongwoon,dw9817-vcm";
					status = "okay";
					reg = <0x0c>;
					phandle = <0xf4>;
					VDD-supply = <&cam1_reg>;
				};

				imx708@1a {
					compatible = "sony,imx708";
					reg = <0x1a>;
					lens-focus = <0xf4>;
					clock-names = "inclk";
					clocks = <&cam1_clk>;
					rotation = <0xb4>;
					orientation = <0x02>;
					//vana1-supply = <0xe9>;
					//vdig-supply = <0xed>;
					//vana2-supply = <0xed>;
					vana1-supply = <&cam1_reg>;
					vdig-supply = <&cam_dummy_reg>;
					vana2-supply = <&cam_dummy_reg>;
					vddl-supply = <&cam_dummy_reg>;
					status = "okay";
					phandle = <0xf3>;

				imx708_port:	port {

				imx708_endpoint:		endpoint {
							data-lanes = <0x01 0x02>;
							clock-noncontinuous;
							clock-lanes = <0x00>;
							// remote-endpoint = <0xf5>;
							remote-endpoint = <&csi1_endpoint>;
							phandle = <0xf6>;
							link-frequencies = <0x00 0x1ad27480>;
						};
					};
				};

			};

		};

		i2c0if: i2c@7e205000 {
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			compatible = "brcm,bcm2711-i2c\0brcm,bcm2835-i2c";
			reg = <0x7e205000 0x200>;
			interrupts = <0x00 0x75 0x04>;
			clocks = <&cprman 0x14>;
			clock-frequency = <0x186a0>;
			status = "okay";
			phandle = <0x32>;
		};
// Camera
		csi1: csi@7e801000 {
			#address-cells = <0x01>;
			#clock-cells = <0x01>;
			compatible = "brcm,bcm2835-unicam";
			reg = <0x7e801000 0x800 0x7e802004 0x04>;
			power-domains = <&power 0x0d>;
			brcm,num-data-lanes = <0x02>;
			clock-names = "lp\0vpu";
			clocks = <&cprman 0x2e &firmware_clocks 0x04>;
			interrupts = <0x00 0x67 0x04>;
			status = "okay";
			phandle = <0x50>;
			brcm,media-controller;

		csi1_port:	port {

		csi1_endpoint:		endpoint {
					data-lanes = <0x01 0x02>;
					clock-noncontinuous;
					clock-lanes = <0x00>;
					//remote-endpoint = <0xf6>;
					remote-endpoint = <&imx708_endpoint>;
					phandle = <0xf5>;
				};
			};
		};


// PWM


	pwm:	pwm@7e20c000 {
			assigned-clocks = <&cprman 0x1e>;
			assigned-clock-rates = <0x2faf080>;
			clocks = <&cprman 0x1e>;
			#pwm-cells = <0x03>;
			compatible = "brcm,bcm2835-pwm";
			status = "disabled";
			reg = <0x7e20c000 0x28>;
			phandle = <0xc1>;
		};

	pwm1:	pwm@7e20c800 {
			pinctrl-names = "default";
			pinctrl-0 = <&pwm1_0_gpio40 &pwm1_1_gpio41>;
			assigned-clocks = <&cprman 0x1e>;
			assigned-clock-rates = <0x2faf080>;
			clocks = <&cprman 0x1e>;
			#pwm-cells = <0x03>;
			compatible = "brcm,bcm2835-pwm";
			status = "disabled";
			reg = <0x7e20c800 0x28>;
			phandle = <0xd5>;
		};

//================ END SOC ======================
	};



 scb:	scb@7c000000 {
		compatible = "simple-bus";
		#address-cells = <0x02>;
		#size-cells = <0x02>;
		dma-ranges = <0x00 0x00 0x00 0x00 0x04 0x00>;
		ranges = <0x00 0x7c000000 0x00 0xfc000000 0x00 0x3800000 0x00 0x40000000 0x00 0xff800000 0x00 0x800000 0x06 0x00 0x06 0x00 0x00 0x40000000 0x00 0x00 0x00 0x00 0x00 0xfc000000>;
		phandle = <0xe5>;

	genet:	ethernet@7d580000 {
			local-mac-address = [dc a6 32 a3 9c 57];
			compatible = "brcm,bcm2711-genet-v5\0brcm,genet-v5";
			reg = <0x00 0x7d580000 0x00 0x10000>;
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			interrupts = <0x00 0x9d 0x04 0x00 0x9e 0x04>;
			status = "okay";
			phy-handle = <&phy>;
			phy-mode = "rgmii-rxid";
			phandle = <0xe6>;

		genet_mdio:	mdio@e14 {
				compatible = "brcm,genet-mdio-v5";
				reg = <0xe14 0x08>;
				reg-names = "mdio";
				#address-cells = <0x00>;
				#size-cells = <0x01>;
				phandle = <0xe7>;

			phy:	ethernet-phy@1 {
					reg = <0x01>;
					led-modes = <0x00 0x08>;
					phandle = <0x40>;
				};
			};
		};
	};

};
