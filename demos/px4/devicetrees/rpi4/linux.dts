emmc2bus:	emmc2bus {
		dma-ranges = <0x00 0x00 0x00 0x00 0xfc000000>;
		#address-cells = <0x02>;
		#size-cells = <0x01>;
		compatible = "simple-bus";
		ranges = <0x00 0x7e000000 0x00 0xfe000000 0x1800000>;
		phandle = <0xe3>;

/**< SD card reader:  
 * cat /sys/block/mmcblk0/queue/scheduler 
 * none [mq-deadline] kyber bfq 
 */
emmc2:		mmc@7e340000 {
			compatible = "brcm,bcm2711-emmc2";
			reg = <0x00 0x7e340000 0x100>;
			mmc-ddr-3_3v;
			interrupts = <0x00 0x7e 0x04>;
			clocks = <&cprman 0x33>;
			broken-cd;
			vqmmc-supply = <&sd_io_1v8_reg>;
			vmmc-supply = <&sd_vcc_reg>;
			status = "okay";
			phandle = <0x57>;
		};
	};


 soc:	soc {
		compatible = "simple-bus";
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		ranges = <0x7e000000 0x00 0xfe000000 0x1800000 0x7c000000 0x00 0xfc000000 0x2000000 0x40000000 0x00 0xff800000 0x800000>;
		dma-ranges = <0xc0000000 0x00 0x00 0x40000000 0x7c000000 0x00 0xfc000000 0x3800000>;
		phandle = <0x5f>;


		watchdog@7e100000 {
			system-power-controller;
			#reset-cells = <0x01>;
			clock-names = "v3d\0peri_image\0h264\0isp";
			reg-names = "pm\0asb\0rpivid_asb";
			clocks = <&cprman 0x15 &cprman 0x1d &cprman 0x17 &cprman 0x16>;
			#power-domain-cells = <0x01>;
			compatible = "brcm,bcm2711-pm\0brcm,bcm2835-pm-wdt";
			reg = <0x7e100000 0x114 0x7e00a000 0x24 0x7ec11000 0x20>;
			phandle = <0x49>;
		};


