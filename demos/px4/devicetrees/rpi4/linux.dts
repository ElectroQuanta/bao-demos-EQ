/dts-v1/;

/ {
	#address-cells = <0x02>;
	#size-cells = <0x01>;
	interrupt-parent = <&gicv2>;
	memreserve = <0x3b400000 0x4c00000>;
	model = "Raspberry Pi 4 Model B Rev 1.5";
	serial-number = "100000000a471c8d";
	compatible = "raspberrypi,4-model-b\0brcm,bcm2711";


	sd_vcc_reg: regulator-sd-vcc {
		regulator-max-microvolt = <0x325aa0>;
		regulator-boot-on;
		gpio = <&expgpio 0x06 0x00>;
		enable-active-high;
		regulator-min-microvolt = <0x325aa0>;
		regulator-name = "vcc-sd";
		compatible = "regulator-fixed";
		phandle = <0x38>;
	};

	fixedregulator_3v3 {
		regulator-max-microvolt = <0x325aa0>;
		regulator-always-on;
		regulator-min-microvolt = <0x325aa0>;
		regulator-name = "3v3";
		compatible = "regulator-fixed";
		phandle = <0xef>;
	};

	fixedregulator_5v0 {
		regulator-max-microvolt = <0x4c4b40>;
		regulator-always-on;
		regulator-min-microvolt = <0x4c4b40>;
		regulator-name = "5v0";
		compatible = "regulator-fixed";
		phandle = <0xf0>;
	};

	gpu {
		compatible = "brcm,bcm2711-vc5";
		status = "okay";
		raspberrypi,firmware = <&firmware>;
		phandle = <0xe2>;
	};

	sd_io_1v8_reg: regulator-sd-io-1v8 {
		regulator-max-microvolt = <0x325aa0>;
		regulator-boot-on;
		regulator-settling-time-us = <0x1388>;
		regulator-always-on;
		regulator-min-microvolt = <0x1b7740>;
		regulator-name = "vdd-sd-io";
		compatible = "regulator-gpio";
		status = "okay";
		states = <0x1b7740 0x01 0x325aa0 0x00>;
		phandle = <0x37>;
		gpios = <&expgpio 0x04 0x00>;
	};

/**< AXI */
//	axi {
//		vc_mem {
//			reg = <0x3ec00000 0x40000000 0xc0000000>;
//		};
//	};

	zone_dma {
		dma-ranges = <0x00 0x00 0x00 0x40000000>;
		#address-cells = <0x01>;
		#size-cells = <0x01>;
	};

/**< V3DBUS */

	v3dbus: v3dbus {
		dma-ranges = <0x00 0x00 0x00 0x04 0x00>;
		#address-cells = <0x01>;
		#size-cells = <0x02>;
		compatible = "simple-bus";
		ranges = <0x7c500000 0x00 0xfc500000 0x00 0x3300000 0x40000000 0x00 0xff800000 0x00 0x800000>;
		phandle = <0xf1>;

	v3d:	v3d@7ec04000 {
			power-domains = <0x49 0x01>;
			reg-names = "hub\0core0";
			resets = <0x49 0x00>;
			interrupts = <0x00 0x4a 0x04>;
			clocks = <&firmware_clocks 0x05>;
			compatible = "brcm,2711-v3d";
			status = "okay";
			reg = <0x7ec00000 0x00 0x4000 0x7ec04000 0x00 0x4000>;
			phandle = <0xf2>;
			clocks-names = "v3d";
		};
	};

 aliases:	aliases {
		serial0 = &uart0;
		serial1 = &uart1;
		serial5 = &uart5;
		ethernet0 = &genet;
		i2c = &i2c_arm;
		i2c1 = &i2c_arm;
		i2c10 = &i2c_csi_dsi;
		//cam0_reg = &cam_dummy_reg;
		vchiq = &mailbox;
		mmc0 = &emmc2;
		phandle = <0xcc>;
	};

 chosen:	chosen {
		stdout-path = "serial5:115200n8";
		bootargs = "coherent_pool=1M earlycon console=serial5,115200 clk_ignore_unused ip=192.168.42.15 carrier_timeout=0 isolcpus=2";
		phandle = <0x17>;

		kaslr-seed = <0x00 0x00>;
		//bootloader {
		//	capabilities = <0x7f>;
		//	build-timestamp = <0x63bef4a4>;
		//	tryboot = <0x00>;
		//	rsts = <0x1000>;
		//	update-timestamp = <0x00>;
		//	version = "8ba17717fbcedd4c3b6d4bce7e50c7af4155cba9";
		//	boot-mode = <0x01>;
		//	partition = <0x00>;
		//};

	};

 cpus:	cpus {
		#size-cells = <0x00>;
		#address-cells = <0x01>;
		phandle = <0x2b>;

	cpu0:	cpu@0 {
			compatible = "arm,armv8";
			device_type = "cpu";
			enable-method = "psci";
			reg = <0x00>;
			phandle = <0x39>;
		};

	cpu1:	cpu@1 {
			compatible = "arm,armv8";
			device_type = "cpu";
			enable-method = "psci";
			reg = <0x01>;
			phandle = <0x3a>;
		};

	cpu2:	cpu@2 {
			compatible = "arm,armv8";
			device_type = "cpu";
			enable-method = "psci";
			reg = <0x02>;
			phandle = <0x3b>;
		};

	cpu3:	cpu@3 {
			compatible = "arm,armv8";
			device_type = "cpu";
			enable-method = "psci";
			reg = <0x03>;
			phandle = <0x3c>;
		};
	};

	psci {
		//compatible = "arm,psci-0.2";
		compatible = "arm,psci-1.0";
		method = "smc";
	};

	memory@0 {
		device_type = "memory";
		reg = <0x00 0x00 0x3b400000 0x00 0x40000000 0xbc000000 0x01 0x00 0x80000000 0x01 0x80000000 0x80000000>;
//		reg = <0x00 0x40000000 0xbc000000>;
	};

